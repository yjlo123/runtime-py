    } else {
        print(say_hi(name))
    }
}

class Counter {
    name = nil
    count = 0

    func init(name) {
        this.name = name
    }

    func increase() {
        this.count += 1
    }

    func to_str() {
        return this.name + ' ' + this.count
    }
}

c1 = Counter('test')
c1.increase()
c1.increase()
print(c1.to_str())
