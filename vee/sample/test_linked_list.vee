class Node {
    value = nil
    next = nil

    func init(val) {
        this.value = val
    }

    func print_me() {
        print(this.value)
        if this.next != nil {
            this.next.print_me()
        }
    }
}


node1 = Node(4)
node1.value = 6

print(node1.value)

node2 = Node(5)

node1.next = node2

print(node1.next.value)

print(node1)

node2.next = Node(89)

node1.print_me()

print('==============')

class List {
    head = nil
    tail = nil

    func push(node) {
        if this.head == nil {
            this.head = node
            this.tail = node
        } else {
            this.tail.next = node
            this.tail = node
        }
        this
    }

    func print_me() {
        if this.head != nil {
            this.head.print_me()
        }
    }
}

list = List()
list.print_me()
list.push(Node(3)).push(Node(2)).push(Node(8)).push(Node(6))
list.print_me()
